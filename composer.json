{
    "name": "taquillacom/doctrine-annotation-reader",
    "description": "Provides a cleaner API for reading Doctrine annotations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Carlos",
            "email": "jcvallejo@taquilla.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "doctrine/annotations": "^1.2",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/cache": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tebru\\AnnotationReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tebru\\AnnotationReader\\Test\\": "tests/"
        }
    }
}
